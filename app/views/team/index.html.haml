%div.section

  %div.contact-btn{ "id" => "contact-us" } contact us
  %div.success-btn{ "id" => "success" } success!

  = render 'contacts/form'

  %div.dropdown.hamburger.hamburger--3dx{"tabindex" => "0", "role" => "button"}
    %div.hamburger-box
      %div.hamburger-inner
    %div.dropdown-content
      %a{ 'href' => '/portfolio'}
        %span.link-underline clients
      %a{ 'href' => '/team'}
        %span.link-underline about us
      %a{ 'href' => '/'}
        %span.link-underline home




  .container

    %div.row.portfolio-row
      .col-md-2.col-md-offset-3.col-xs-1.col-xs-offset-1.back-img
        %a{ :href => '/' }
          = image_tag 'back-btn.png', :class => 'img-responsive, back-btn-heading'
      .col-md-2.col-xs-7.about-us
        %p.about-us-title about us

    %div.row.team-row
      %div.col-md-2.col-md-offset-3.col-xs-12
        %div.pic.profile.darren{ 'href' => '#darren', 'data-toggle' => 'modal'}
      %div.col-md-2.col-xs-12
        %div.pic.profile.christopher{ 'href' => '#christopher', 'data-toggle' => 'modal'}
      %div.col-md-2.col-xs-12
        %div.pic.profile.kimberley{ 'href' => '#kimberley', 'data-toggle' => 'modal'}

    %div.row.team-row
      %div.col-md-2.col-md-offset-3.col-xs-12
        %div.pic.stu.profile{ 'href' => '#stu', 'data-toggle' => 'modal'}
      %div.col-md-2.col-xs-12
        %div.pic.profile.stephanie{ 'href' => '#stephanie', 'data-toggle' => 'modal'}
      %div.col-md-2.col-xs-12
        %div.pic.profile.dave{ 'href' => '#dave', 'data-toggle' => 'modal'}

    %div.row.team-row
      %div.col-md-2.col-md-offset-3.col-xs-12
        %div.pic.profile.andrew{ 'href' => '#andrew', 'data-toggle' => 'modal'}
      %div.col-md-2.col-xs-12
        %div.pic.profile.russell{ 'href' => '#russell', 'data-toggle' => 'modal'}
      %div.col-md-2.col-xs-12
        %div.pic.trent.profile{ 'href' => '#trenton', 'data-toggle' => 'modal'}

